{
  "name": "watchboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "doppler run --print-config --command=\"react-scripts build\"",
    "eject": "react-scripts eject",
    "start": "doppler run --print-config --command=\"react-scripts start\"",    
    "start:dev": "doppler run --print-config --config dev --command=\"react-scripts start\"",    
    "start:prod": "doppler run --print-config --config prod --command=\"react-scripts start\"",
    "test": "react-scripts test"
  },
